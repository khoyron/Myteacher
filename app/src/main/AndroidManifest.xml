<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.khoiron.myteacher.guru.lest.privates">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.home.Home"
            android:theme="@style/AppThemee">
        </activity>
        <activity android:name=".activity.login.SplashScreen"
            android:theme="@style/AppThemee">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.login.LoginActivity"
            android:theme="@style/AppThemee">

        </activity>
        <activity android:name=".activity.login.RegisterActivity"
            android:theme="@style/AppThemee">
        </activity>
        <activity android:name=".activity.home.SearchActivity"
            android:theme="@style/AppThemee">
        </activity>
        <activity android:name=".activity.service.CreateServiceActivity"
            android:theme="@style/AppThemee">
        </activity>
        <activity android:name=".activity.service.DetailServiceActivity"
            android:theme="@style/AppThemee">
        </activity>
        <activity android:name=".activity.service.DetailCategoryActivity"
            android:theme="@style/AppThemee">
        </activity>

    </application>

</manifest>